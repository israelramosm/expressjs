{
  "name": "sheloapp",
  "version": "0.1.0",
  "private": true,
  "author": "Israel Ramos",
  "scripts": {
    "db-init": "mongod --auth --port 27017 --dbpath C:\\dbs\\shelodb",
    "start": "nodemon ./dist/server.js",
    "build": "babel ./server --out-dir ./dist && npm run build-app",
    "watch": "cpx env.globals dist && babel server --copy-files --watch --out-dir dist",
    "test": "jest --detectOpenHandles --forceExit --coverage --verbose",
    "start-app": "react-scripts start",
    "build-app": "react-scripts build && cpx build/**/* dist/public/",
    "watch-app": "not implemented",
    "test-app": "react-scripts test",
    "eject-app": "react-scripts eject"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.0",
    "cpx": "^1.5.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "express-winston": "^3.0.1",
    "modern-scripts": "2.2.0",
    "mongoose": "^5.3.13",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "dotenv": "^6.1.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.10",
    "mongodb-memory-server": "^2.7.3",
    "nodemon": "^1.18.6",
    "supertest": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
