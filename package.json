{
  "name": "sheloapp",
  "version": "0.1.0",
  "private": true,
  "author": "Israel Ramos",
  "scripts": {
    "db-init": "mongod --auth --port 27017 --dbpath C:\\dbs\\shelodb",
    "clean": "rimraf dist",
    "start": "node ./dist/server.js",
    "build:dev": "npm run clean && npm run build:dev-server && npm run build:dev-client",
    "build:dev-server": "webpack --mode development --progress --colors --config ./config/webpack.server.config.js",
    "build:dev-client": "webpack --progress --colors --config ./webpack.dev.config.js",
    "build:prod": "npm run clean && npm run build:prod-server && npm run build:prod-client",
    "build:prod-server": "webpack --mode production --progress --colors --config ./config/webpack.server.config.js",
    "build:prod-client": "webpack --progress --colors --config ./webpack.prod.config.js"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "express-winston": "^3.0.1",
    "mongoose": "^5.3.13",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.5",
    "mongodb": "^3.1.10",
    "mongodb-memory-server": "^2.7.3",
    "nodemon": "^1.18.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
